@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://rpcw.di.uminho.pt/2025/typec/> .

<http://rpcw.di.uminho.pt/2025/typec> rdf:type owl:Ontology .


#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
                                                     rdfs:domain :Text ;
                                                     rdfs:range :Author .


###  http://www.co-ode.org/ontologies/ont.owl#hasTopic
:hasTopic rdf:type owl:ObjectProperty ;
                                                    rdfs:domain :Text ;
                                                    rdfs:range :Topic .


#################################################################
#    Data properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#auth_name
:auth_name rdf:type owl:DatatypeProperty ;
                                                     rdfs:domain :Author ;
                                                     rdfs:range xsd:string .


###  http://www.co-ode.org/ontologies/ont.owl#subject
:subject rdf:type owl:DatatypeProperty ;
                                                   rdfs:domain :Topic ;
                                                   rdfs:range xsd:string .


###  http://www.co-ode.org/ontologies/ont.owl#title
:title rdf:type owl:DatatypeProperty ;
                                                 rdfs:domain :Text ;
                                                 rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#Author
:Author rdf:type owl:Class ;
                                                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :auth_name ;
                                                                    owl:someValuesFrom xsd:string
                                                                  ] .


###  http://www.co-ode.org/ontologies/ont.owl#Text
:Text rdf:type owl:Class ;
                                                owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :hasAuthor ;
                                                                      owl:someValuesFrom :Author
                                                                    ] ,
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :hasTopic ;
                                                                      owl:someValuesFrom :Topic
                                                                    ] ,
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :title ;
                                                                      owl:someValuesFrom xsd:string
                                                                    ] .


###  http://www.co-ode.org/ontologies/ont.owl#Topic
:Topic rdf:type owl:Class ;
                                                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :subject ;
                                                                   owl:someValuesFrom xsd:string
                                                                 ] .


